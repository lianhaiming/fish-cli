#!/usr/bin/env node

const program = require('commander');
/**
 * Usage
 */

program
    .version(require('../package.json'))
    .usage('<commander> [options]')
    .command('react [filename]', 'generate a new project of react').alias('re')
    // .command('angular [filename]', 'generate a new project of angular').alias('ng')
    // .command('vue [filename]', 'generate a new project of vue')
    .parse(process.argv)